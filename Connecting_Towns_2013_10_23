#solution toward https://www.hackerrank.com/challenges/connecting-towns
#written in Python on 2015-01-21
def main():
    n = int(raw_input())
    for _ in range(n):
        town = int(raw_input())
        ar = [int(x) for x in raw_input().strip().split()]
        result = 1
        for i in ar:         
            result *= i
        print result % 1234567

if __name__ == '__main__':
    main()
